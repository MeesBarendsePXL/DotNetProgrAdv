<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	
    <RadialGradientBrush x:Key="DarkRadialBrush">
        <GradientStop Color="Blue"></GradientStop>
        <GradientStop Color="Black" Offset="0.75"></GradientStop>
    </RadialGradientBrush>
    <SolidColorBrush x:Key="DarkSolidBrush" Color="Black"></SolidColorBrush>
    <SolidColorBrush x:Key="FontBrush" Color="White"></SolidColorBrush>
</ResourceDictionary>